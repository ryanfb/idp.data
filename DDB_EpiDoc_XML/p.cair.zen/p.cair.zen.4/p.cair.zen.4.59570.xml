    <lb n="6"/>ὀλυρῶν <num value="331">τλα</num> <num value="1/2">𐅵</num>, καὶ τοῦ Παχὼνς <gap reason="lost" atLeast="1" atMost="2" unit="character"/> <gap reason="lost" extent="unknown" unit="character"/> 

