    <lb n="1"/><app type="editorial"><lem resp="BL cf. 7.43"><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τ</supplied><unclear>υ</unclear>γχάν<unclear>ῳ</unclear> <supplied reason="lost">μ</supplied><unclear>έ</unclear>λλουσ<supplied reason="lost">α </supplied><gap reason="lost" quantity="4" unit="character"/>ο</lem><rdg>γχανο<gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>λλουσ<gap reason="lost" quantity="5" unit="character"/>ο</rdg></app> γὰρ <gap reason="illegible" quantity="2" unit="character"/>τας ἐκεῖ ο<gap reason="lost" quantity="4" unit="character"/>αστηρ <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="4" unit="character"/>ξην 

