    <lb n="16"/><gap reason="lost" quantity="4" unit="character"/> ἀρνίων καὶ <expan>ἡψημένω<ex>ν</ex></expan> 

    <lb n="17"/><supplied reason="lost" cert="low">ἀριθ</supplied><unclear>μ</unclear>ῷ <num value="106">ρϛ</num> χωροῦν

    <lb n="18" break="no"/><supplied reason="lost">τα εἰς</supplied> κατασκευὴν ὅπλων 

    <lb n="19"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>κ<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="20"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="3" unit="character"/>αν προκειμεν<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

