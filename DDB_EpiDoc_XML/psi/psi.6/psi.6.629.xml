    <lb n="6"/><num value="9">θ</num> <app type="editorial"><lem resp="BL 6.178"><expan>μν<ex>ῶν</ex></expan></lem><rdg><expan><unclear>ἀ</unclear>ν<ex>ὰ</ex></expan></rdg></app>  <num value="4">δ</num> <num value="1/2">𐅵</num> 
