    <lb n="13"/><expan>πράκ<ex>τωρ</ex></expan> <expan>ἀργ<ex>υρικῶν</ex></expan> Αρ<gap reason="illegible" quantity="4" unit="character"/> <expan>δ<ex>ιὰ</ex></expan> <expan>Ἑρμ<unclear>ίο</unclear><ex>υ</ex></expan> <expan><unclear>γρα</unclear><ex>μματέως</ex></expan> <expan>σεση<ex>μείωμαι</ex></expan>. 

