    <lb n="4"/>παρὰ τῶν ἀπὸ κώμης Σεφὼ <supplied reason="lost" cert="low"><num value="7">ζ</num> πάγου</supplied> 

    <lb n="5"/>διʼ ἡμῶν <supplied reason="lost">τ</supplied>ῶν δημοσίων <unclear>Α</unclear><supplied reason="lost">ὐρηλίων</supplied> 

    <lb n="6"/>Ψεναμούνιος Ψ<unclear>όϊ</unclear>τος <choice><reg>μείζ<supplied reason="lost">ονος</supplied></reg><orig>μίζ<supplied reason="lost">ονος</supplied></orig></choice><supplied reason="lost"> τῆς</supplied> 

