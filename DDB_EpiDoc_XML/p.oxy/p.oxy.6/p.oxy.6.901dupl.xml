         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/><hi rend="diaeresis">ὑ</hi>πατείας Ο<supplied reason="lost">ὐιβίου</supplied> Νεπωτιανοῦ καὶ Τετ<g type="apostrophe"/><unclear>τ</unclear>ίου Φακούνδου 

    <lb n="2"/>τῶν λαμπροτάτων Π<supplied reason="lost">α</supplied>χὼν <num value="6">ϛ</num>. 

    <lb n="3"/>Φλαου<hi rend="diaeresis">ί</hi><supplied reason="lost">ῳ</supplied> <hi rend="diaeresis">Ἰ</hi>ουλιανῷ διοικοῦντι <app type="editorial"><lem resp="BL 9.181; cf. 3.133"><unclear>συ</unclear>νδικίαν</lem><rdg><unclear>ἐκ</unclear>δικίαν</rdg></app> Ὀξυρυγχίτου 

    <lb n="4"/>παρὰ Αὐρηλίας Ἀλλοῦτος Θωνίου ἀπὸ κώμης Τααμπέμο<supplied reason="lost">υ</supplied> 

    <lb n="5"/><num value="5">ε</num> πάγου. <choice><reg>ἑσπεριναῖς</reg><orig>ἑσπερινε͂ς</orig></choice> <choice><reg>ὥραις</reg><orig>ὥρες</orig></choice> τῇ χθὲς ἡμέρᾳ ἡμέτεροι <choice><reg>χοῖροι</reg><orig>χῦροι</orig></choice> 

    <lb n="6"/>δύο <num value="2"/> τὴν ὁρμὴν ποιούμενοι ἐπὶ ἡμέτερον ἔδαφος 

    <lb n="7"/><choice><reg>γενόμενοι</reg><orig>γενώμενοι</orig></choice> ἐν ὑδραγωγίῳ μηχανῆς τῶν ἡμετέρων 

    <lb n="8"/><app type="editorial"><lem resp="BL 9.181"><choice><reg>πεδίων</reg><orig>πεδων</orig></choice></lem><rdg>παίδων</rdg></app> καὶ <unclear>Π</unclear>αβάνου τινὸς ἀπὸ τῆς αὐτῆς <app type="editorial"><lem resp="BL 9.181">κώμης</lem><rdg>κώμης <supplied reason="lost">καὶ</supplied></rdg></app>. 

    <lb n="9"/>ὁ <choice><reg>προειρημ<supplied reason="lost">ένος</supplied></reg><orig>προ<hi rend="diaeresis">ι</hi>ρημ<supplied reason="lost">ένος</supplied></orig></choice><supplied reason="lost"> Π</supplied><unclear>α</unclear>βᾶνος παρατυχὼν ἔχων μετὰ 

    <lb n="10"/><choice><reg>χεῖρας</reg><orig>χῖρας</orig></choice> <app type="editorial"><lem resp="BL 9.181">ξοίδιον ἀνελεῖν</lem><rdg><choice><reg>ξύλιον</reg><orig>ξοί<unclear>λ</unclear>ιο<unclear>ν</unclear></orig></choice> <gap reason="illegible" quantity="4" unit="character"/>ιν</rdg></app> τοὺς <choice><reg>χοίρους</reg><orig>χύρους</orig></choice> βουλόμεν<supplied reason="lost">ο</supplied>ς 

    <lb n="11"/>καὶ τοῦ<unclear>τ</unclear>ο <unclear>μ</unclear><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="5" unit="character"/><unclear>ω</unclear>ς μηδαμῶς ἀδικηθεὶ<supplied reason="lost">ς</supplied> 

    <lb n="12"/>ὑπὸ τῶν <choice><reg>χοίρ<supplied reason="lost">ων</supplied></reg><orig>χ<unclear>ύρ</unclear><supplied reason="lost">ων</supplied></orig></choice> <app type="editorial"><lem resp="BL 9.181">ἀποσχόμενος</lem><rdg><supplied reason="lost">ἀλ</supplied><unclear>λὰ</unclear> ἐχόμενος</rdg></app> τῆς πρὸς τοὺς 

    <lb n="13"/><choice><reg>χοίρους</reg><orig>χύρους</orig></choice> ε<gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="2" unit="character"/><supplied reason="lost"> ὅτι</supplied> <unclear>ἐμοὶ</unclear> ἐπελήλυθαν βουλόμενος 

    <lb n="14"/><app type="editorial"><lem resp="BL 9.182"><unclear>μο</unclear>ι κατα<certainty match=".." locus="value"/></lem><rdg><unclear>ο</unclear>ικατα</rdg></app> <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="15" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ως <choice><reg>εἰ</reg><orig><hi rend="diaeresis">ἰ</hi></orig></choice> μὴ ἔκ<g type="apostrophe"/> τινος 

    <lb n="15"/><app type="editorial"><lem resp="BL 9.182"><choice><reg><supplied reason="lost">προ</supplied>νοίας</reg><orig><supplied reason="lost">προ</supplied>νύας</orig></choice></lem><rdg><choice><reg><supplied reason="lost">ἀπο</supplied>νοίας</reg><orig><supplied reason="lost">ἀπο</supplied>νύας</orig></choice></rdg></app> τὴν <gap reason="lost" quantity="8" unit="character"/><unclear>π</unclear><gap reason="illegible" quantity="1" unit="character"/> <supplied reason="lost">ἀ</supplied><unclear>λ</unclear>λʼ οὖν τῶν <choice><reg>βοῶν</reg><orig>βωῶν</orig></choice> 

    <lb n="16"/><gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>ον</unclear> <unclear>α</unclear>νκα<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="2" unit="character"/><unclear>η</unclear>μην <hi rend="diaeresis">ὑ</hi>πʼ αὐτοῦ 

    <lb n="17"/><gap reason="lost" quantity="23" unit="character"/>κόψας ὡς ἐκ τῶν 

    <lb n="18"/><gap reason="lost" quantity="18" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>οις</unclear> τινὰς <choice><reg>παράγεται</reg><orig>παράγε<unclear>τ</unclear>ε</orig></choice> 

    <lb n="19"/><gap reason="lost" quantity="26" unit="character"/> θρασύτητ<unclear>ι</unclear> <gap reason="lost" quantity="3" unit="character"/> 

