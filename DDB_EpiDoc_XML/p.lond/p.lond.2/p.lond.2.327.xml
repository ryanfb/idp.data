    <lb n="16"/><expan>Μεχ<ex>εὶρ</ex></expan> <num value="3">γ</num> 

    <lb n="17"/><expan>ἀπεγρ<ex>άφη</ex></expan> παρὰ <expan>βασιλ<ex>ικῷ γραμματεῖ</ex></expan> <expan>κάμ<ex>ηλος</ex></expan> <num value="1">α</num> 

