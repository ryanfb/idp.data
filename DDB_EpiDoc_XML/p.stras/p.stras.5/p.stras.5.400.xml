    <lb n="1"/><g type="chirho"/> μαθέτω ὁ ἐμὸς <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>καὶ προσδοκου<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>ἐὰν οῦ̓ν ἡδέως ἔχεται <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>ξηρανθοῦσιν καὶ π<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>τῷ ὑμετέρῳ μετε<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>βάσεως τοῦ <hi rend="diaeresis">ὕ</hi>δατος <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>μαθέτω δὲ καὶ τοῦτο  <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>τῶν πληθῶν τῶν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>καθʼ ἑκάστην μη<unclear>χ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>ἵνα μὴ ἀνατραπ<gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>τοῦ πιτ<g type="apostrophe"/>τακίου αὐτ<unclear>οῦ</unclear> <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

