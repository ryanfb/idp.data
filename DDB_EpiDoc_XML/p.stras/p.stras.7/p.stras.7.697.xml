    <lb n="5"/><gap reason="lost" quantity="18" unit="character"/>ετέ<unclear>ρ</unclear>ας. κα<unclear>ὶ</unclear> γὰρ κα<gap reason="lost" quantity="6" unit="character"/> 

    <lb n="6"/><gap reason="lost" quantity="18" unit="character"/>αγι τῇ ἀγ<gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="4" unit="character"/>η<gap reason="lost" quantity="6" unit="character"/> 

