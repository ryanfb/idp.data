    <lb n="11"/><gap reason="lost" quantity="3" unit="character"/>αν πα<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>αξι<unclear>α</unclear> Λεοντᾶτος <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> 

