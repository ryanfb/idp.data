    <lb n="6"/>τέσσερα <expan>γ<ex>ίνεται</ex></expan> <expan>δίχω<ex>ρα</ex></expan> <num value="4">δ</num> 

    <lb n="7"/>ἀλλὰ καλὰ καὶ εἰς <gap reason="illegible" quantity="4" unit="character"/> 

    <lb n="8"/><gap reason="illegible" quantity="2" unit="character"/>τα <choice><reg>θέλεις</reg><orig>θέλις</orig></choice> ἀλλα <gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><gap reason="lost" quantity="2" unit="character"/>εμε<gap reason="illegible" quantity="1" unit="character"/>ει<gap reason="illegible" quantity="1" unit="character"/>καπετ<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> 

