    <lb n="1,msup"/><figure><figDesc>stamp</figDesc></figure> 

    <lb n="1"/>Πανίσκωι συγγενεῖ καὶ στρατη<supplied reason="lost">γῶι καὶ ἐπὶ τῶν</supplied> 

    <lb n="2"/>προσόδων <space extent="unknown" unit="character"/> 

    <lb n="3"/>παρὰ <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="8" unit="character"/> 

    <lb n="4"/>ἡ<unclear>γε</unclear>μόνι <gap reason="illegible" quantity="1" unit="character"/> π<unclear>υκ</unclear>νότερ<supplied reason="lost">ον </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>ἐπερχομένων <unclear>ἄλλων</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>α<unclear>πί</unclear>ωι δι<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>μένον ὑπʼ ἐμοῦ <unclear>χ</unclear><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>Τ<unclear>α</unclear>μφνοῦθιν <unclear>ἀπὸ</unclear> <unclear>σίτων</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>νοντες καὶ τὸν λοιπὸν <unclear>π</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>σπόρον, <unclear>ὅ</unclear>θ<unclear>ε</unclear>ν το<unclear>ῦ</unclear> <unclear>παρ</unclear>ʼ ἐμο<supplied reason="lost">ῦ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>φύλακος σὺν τῶι παρ<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/><choice><reg>συστρατιώτηι</reg><orig>συνστρατιώτηι</orig></choice> <gap reason="illegible" quantity="2" unit="character"/><unclear>ρ</unclear><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>μένων τούτους κατέλ<unclear>αβον</unclear> <unclear>τὸ</unclear><supplied reason="lost">ν αὐ</supplied>

    <lb n="14" break="no"/>τὸν Θεό<unclear>δωρ</unclear>ο<unclear>ν</unclear> <unclear>τ</unclear><supplied reason="lost">ὸ</supplied>ν ὄντα ἐν τῶι <unclear>τόπ</unclear>ωι 

    <lb n="15"/>ἀπο<unclear>σπῶ</unclear><supplied reason="lost">ν</supplied>τα χόρτον. <choice><reg>οἱ</reg><orig>ο</orig></choice> δʼ ἀπολύσαντες 

    <lb n="16"/>τοῦτον καὶ ἐξ ἐπιστροφῆς <choice><reg>παραγενό<lb n="17" break="no"/>μενοι</reg><orig>παραγενο<lb n="17" break="no"/>μενος</orig></choice> σὺν Πτολεμαίωι Νίκωνος τῶν 

    <lb n="18"/>ἀπὸ τῆς <unclear>μεγά</unclear>λης π<unclear>λ</unclear>α<unclear>τ</unclear>ικ καὶ 

    <lb n="19"/>ἄλλοις ἱππεῦσι καὶ ἐπιθέμενοι τῶι τε 

    <lb n="20"/>φύλακι καὶ τῶι <choice><reg>συστρατιώτηι</reg><orig>συνστρατιώτηι</orig></choice> Ἀπολλ<unclear>ων</unclear>ίωι 

    <lb n="21"/>κατήνεγκαν αὐτῶν πάντα καὶ αὐτοὺς 

    <lb n="22"/>εἰς τὸν περὶ τοῦ ζῆν περιέστησαν κίν

    <lb n="23" break="no"/>δυνον, ἐγβιασάμενοι καὶ ἣν <choice><reg>περιεστάλ<lb n="24" break="no"/>κεσαν</reg><orig>πε<unclear>ριεστάλ</unclear><lb n="24" break="no"/>κεισαν</orig></choice> αὑτῶν οἱ παρʼ ἐμοῦ ὄνον σὺν τῶι 

    <lb n="25"/>ἐπικειμένωι χόρτωι. ἀξιῶ ἐὰν φαίνηται 

    <lb n="26"/>συντάξαι καταστῆσαι <unclear>τούτου</unclear>ς ἐπὶ σὲ 

    <lb n="27"/>καὶ συνέχειν μέχρ<supplied reason="lost">ι</supplied> τοῦ ἐκ<unclear>τεῖσαι</unclear> <unclear>τὴν</unclear> <unclear>γεγο</unclear>

    <lb n="28" break="no"/>νυῖαν ὑπʼ αὐτῶν τοῦ χόρτου καὶ τοῦ σπόρου 

    <lb n="29"/>φθορὰν καὶ περὶ αὐτῶν διαλαβεῖν μισο

    <lb n="30" break="no"/>πονήρως, ἵνʼ ῶ̓ τετευχὼς τῆς σῆς δικαι

    <lb n="31" break="no"/>οσύνης. 
