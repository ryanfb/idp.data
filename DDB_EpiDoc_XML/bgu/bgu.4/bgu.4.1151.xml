    <lb n="28"/>τοῦ <expan>Ἀπολλω<ex>νίου</ex></expan> <expan>ἀμφοτέ<ex>ρων</ex></expan> <expan><unclear>Περσ</unclear><ex>ῶν</ex></expan> <expan><unclear>τῆ</unclear><ex>ς</ex></expan> <expan><unclear>ἐπιγο</unclear><ex>νῆς</ex></expan>. <unclear>περὶ</unclear> <expan>τ<ex>ῶν</ex></expan> <expan><unclear>διεστ</unclear><ex>αμένων</ex></expan> <expan>συνχωρο<ex>ῦσιν</ex></expan> Ἀγαθοκλ<supplied reason="lost">ῆς</supplied> 

    <lb n="29"/>καὶ <expan>Λυκαρί<ex>ων</ex></expan> <expan>ἔχε<ex>ιν</ex></expan> παρὰ <expan>το<ex>ῦ</ex></expan> <expan>Γαίο<ex>υ</ex></expan> <expan>Ἰουλ<ex>ίου</ex></expan> <expan>Φιλ<ex>ίου</ex></expan> <choice><reg><expan>δάνει<ex>ον</ex></expan></reg><orig>δανη</orig></choice> διὰ <expan>τῆ<ex>ς</ex></expan> <expan>Κάστορο<ex>ς</ex></expan> <expan>κολ<ex>λυβιστικῆς</ex></expan> <expan>τραπ<ex>έζης</ex></expan> 

    <lb n="30"/><expan>ἀργυ<ex>ρίου</ex></expan> <expan><unclear>Πτολ</unclear><ex>εμαικοῦ</ex></expan> δραχμῶν ἑβδομήκοντα <num value="70"/>, <app type="editorial"><lem resp="BL 5.15"><expan><unclear>ἄτοκ</unclear><ex>ον</ex></expan> <unclear>ὃ</unclear><certainty match=".." locus="value"/></lem><rdg><unclear>ἃς</unclear> <gap reason="illegible" quantity="4" unit="character"/></rdg></app> <unclear>καὶ</unclear> <expan>ἀποδώσ<ex>ειν</ex></expan> ἐν <expan>μησ<ex>ὶν</ex></expan> 

