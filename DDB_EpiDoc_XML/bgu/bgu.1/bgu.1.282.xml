    <lb n="2"/><gap reason="lost" quantity="14" unit="character"/><supplied reason="lost"> Κ</supplied>άστορ<gap reason="lost" quantity="13" unit="character"/><supplied reason="lost"> λαογρα</supplied>φουμε<supplied reason="lost">ν </supplied><gap reason="lost" quantity="5" unit="character"/> 

    <lb n="3"/><gap reason="lost" quantity="16" unit="character"/> τοῦ αὐτοῦ Ἀρσι<supplied reason="lost">νο</supplied>είτου ἀναγραφομένου α<gap reason="lost" quantity="6" unit="character"/> 

    <lb n="4"/><gap reason="lost" quantity="16" unit="character"/> <app type="editorial"><lem resp="bgu 3 p.2"><choice><reg>συνχωρεῖ</reg><orig>συνχωρῖ</orig></choice></lem><rdg>συνχωρη</rdg></app> <gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" quantity="2" unit="character"/> <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> <app type="editorial"><lem resp="BGU 2 p.355"><supplied reason="lost">πεπρακέ</supplied>

