    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/><expan><unclear>Φ</unclear><supplied reason="lost">λ<ex>αουίῳ</ex></supplied></expan> <unclear>Χρ</unclear><supplied reason="lost">ιστοφόρῳ τῷ</supplied> 

    <lb n="2"/><expan>μεγαλ<ex>οπρεπεσ</ex>τ<ex>άτῳ</ex></expan> <expan>παγάρχ<ex>ῳ</ex></expan> τοῦ <expan>βορρ<ex>ινοῦ</ex></expan> 

    <lb n="3"/>σκέλους ταύτης τῆς <expan>πολ<ex>ι</ex>τ<ex>είας</ex></expan> 

    <lb n="4"/>διʼ ὑμῶν Κοσμᾶ τοῦ <expan>περιβλ<ex>έ</ex>π<ex>του</ex></expan> 

    <lb n="5"/>αὐτοῦ <expan>χαρ<ex>του</ex>λ<ex>αρίου</ex></expan> <g type="unintelligible"/> Ἠλίας <expan>ἀρτ<ex>ο</ex>πρ<ex>ά</ex>τ<ex>ης</ex></expan> 

    <lb n="6"/>υἱὸς Γεωργίου ἀφʼ <expan>Ἡρ<ex>ακλέους</ex></expan> <expan>π<ex>όλεως</ex></expan> 

    <lb n="7"/>ἑξῆς <expan>ὑπογρ<ex>άφων</ex></expan> ἰδίᾳ χειρί. 

    <lb n="8"/>ὁμολογῶ ἐγὼ ὁ <expan>προ<ex>κείμενος</ex></expan> 

    <lb n="9"/>Ἠλίας εἰληφέναι 

    <lb n="10"/>καὶ δεδέχθαι παρʼ ὑμῶν 

    <lb n="11"/>λόγῳ τῆς <choice><reg>ῥόγας</reg><orig>ῥώγας</orig></choice> τοῦ 

    <lb n="12"/><expan>αὐτ<ex>οῦ</ex></expan> <expan>βορρ<ex>ινοῦ</ex></expan> <choice><reg>κλίματος</reg><orig>κλήματος</orig></choice> 

    <lb n="13"/>σίτου ἀρτάβας εἴκοσι 

    <lb n="14"/><expan>γί<ex>νονται</ex></expan> <expan>σί<ex>του</ex></expan> <expan><ex>ἀρτάβαι</ex></expan> <num value="20">κ</num>, καὶ ἑτοίμως 

    <lb n="15"/>ἔχειν με ταύτας <choice><reg>ῥογεῦσαι</reg><orig>ῥωγεῦσαι</orig></choice> 

    <lb n="16"/>ὑμῖν ἐν ψωμίοις 

    <lb n="17"/>ἀνεμποδίστως, ὅτε 

    <lb n="18"/>δήποτε βουληθείητε, 

    <lb n="19"/>καὶ πρὸς ὑμετέραν 

    <lb n="20"/>ἀσφάλειαν πεποίημαι ὑμῖν 

    <lb n="21"/>τὸ παρὸν πρόσγραφον 

    <lb n="22"/><expan>κύρ<ex>ιον</ex></expan> εῖ̓ναι, καὶ <expan>ἐπερ<ex>ωτη</ex>θ<ex>εὶς</ex></expan> <expan>ὡμολ<ex>όγησα</ex></expan> 

    <lb n="23"/>καὶ ἀπέλυσα <g type="stauros"/> <handShift new="m2"/> <g type="stauros"/> ἐγὼ Ἠλίας 

    <lb n="24"/>ἀρτοπράτης υἱὸς 

    <lb n="25"/>Γεωργίου <choice><reg>στοιχεῖ</reg><orig>στιχῖ</orig></choice> μοι 

    <lb n="26"/><handShift new="m3"/><g type="chirho"/> <g type="chirho"/> <foreign xml:lang="grc-Latn">di emu <add place="above"><g type="stauros"/></add> Anup <add place="above"><g type="stauros"/></add> <expan>sumbolaiog<ex>raphu</ex></expan></foreign>

    <lb n="27"/>διʼ ἐμοῦ Ἀνοὺπ <expan>συμβ<ex>ο</ex>λ<ex>αιογράφου</ex></expan> <app type="editorial"><lem resp="BL 8.22"><expan>νομ<ex>ικ</ex>ο<ex>ῦ</ex></expan></lem><rdg/></app> <g type="tachygraphic-marks"/> 

