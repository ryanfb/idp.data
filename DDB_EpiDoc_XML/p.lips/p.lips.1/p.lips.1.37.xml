         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/>μετὰ τὴν ὑπα<supplied reason="lost">τ</supplied>ε<supplied reason="lost">ίαν το</supplied>ῦ δεσπ<supplied reason="lost">ό</supplied>του ἡμῶν Θεοδ<supplied reason="lost">οσίο</supplied>υ τοῦ 

    <lb n="2"/>αἰωνίου Αὐγο<supplied reason="lost">ύσ</supplied>του τὸ <num value="2">β</num> καὶ Φλα<supplied reason="lost">υ</supplied>ί<supplied reason="lost">ο</supplied>υ Κυνηγί<supplied reason="lost">ο</supplied>υ τοῦ λαμ<supplied reason="lost">πρ</supplied>οτάτου. 

    <lb n="3"/>Αὐρηλίῳ Ζηνοδότῳ <gap reason="illegible" quantity="2" unit="character"/><unclear>π</unclear>ιο<supplied reason="lost">υ</supplied> προ<unclear>πολ</unclear>ιτ<unclear>ε</unclear><supplied reason="lost">υο</supplied>μέ<supplied reason="lost">νῳ</supplied> Ἑρμοῦ 

    <lb n="4"/>πόλεως τῆς <supplied reason="lost">λ</supplied>αμπρ<supplied reason="lost">οτ</supplied>άτης ῥιπαρίῳ νομο<supplied reason="lost">ῦ</supplied> Ἑρμουπολίτου 

    <lb n="5"/>παρὰ Φλαυίο<supplied reason="lost">υ Ἰ</supplied>σιδ<supplied reason="lost">ώρ</supplied>ου ἀπὸ <expan>β<ex>ενε</ex>φ<ex>ικιαρίων</ex></expan> τά<supplied reason="lost">ξ</supplied>εως ἡγ<supplied reason="lost">ε</supplied>μονίας 

    <lb n="6"/>Θηβαίδος. φ<unclear>θ</unclear><supplied reason="lost">ά</supplied>σ<supplied reason="lost">ας </supplied><app type="editorial"><lem resp="BL 1.207"><choice><reg><supplied reason="lost">μ</supplied>έντο<supplied reason="omitted">ι</supplied></reg><orig><supplied reason="lost">μ</supplied>έντο</orig></choice> δὴ καὶ πρότερόν <supplied reason="lost">σο</supplied>ι</lem><rdg><supplied reason="lost">μ</supplied>ὲν <unclear>ἔ</unclear>δ<supplied reason="lost">ω</supplied>κα <unclear>λιβέ</unclear>λ<supplied reason="lost">λ</supplied><unclear>ους</unclear> <supplied reason="lost">κα</supplied>ὶ</rdg></app> πολλάκις 

    <lb n="7"/>ᾐτιασάμην Ἰω<supplied reason="lost">νᾶν</supplied> <unclear>Ε</unclear><supplied reason="lost">ῦ̓</supplied><unclear>τ</unclear><supplied reason="lost">ο</supplied>ς καὶ <supplied reason="lost">Ἁ</supplied>τρῆτ<supplied reason="lost">α</supplied> Ἑρμίνου <unclear>ποι</unclear>μένας 

    <lb n="8"/>ἀπὸ κώμης Τελ<supplied reason="lost">βώ</supplied><unclear>νθ</unclear><supplied reason="lost">ε</supplied>ως <unclear>κακ</unclear><supplied reason="lost">ο</supplied><unclear>ύργ</unclear><supplied reason="lost">ου</supplied>ς ὡς <app type="editorial"><lem resp="BL 1.207">ἐ<supplied reason="lost">ξ</supplied>αφέντας</lem><rdg><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="2" unit="character"/>αφεντασ</rdg></app> 

    <lb n="9"/>τὰς ἀγέλας τῶν <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="5" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>τω<unclear>ν</unclear> <app type="editorial"><lem resp="BL 1.207">αὐτῶν</lem><rdg><gap reason="illegible" quantity="2" unit="character"/><unclear>τ</unclear>ω<unclear>ν</unclear></rdg></app> οὐ<supplied reason="lost">χ ἅ</supplied>παξ <supplied reason="lost">οὐ</supplied> <unclear>δε</unclear>ύτερον 

    <lb n="10"/>οὐ τρίτον εἰς τοὺς <app type="editorial"><lem resp="BL 1.207">ἡ<supplied reason="lost">μετ</supplied>έρ<supplied reason="lost">ου</supplied>ς καρποὺς</lem><rdg><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ε<gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>ς</unclear> κερτ<unclear>ομίας</unclear></rdg></app> <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="1" unit="character"/> <unclear>α</unclear>ὐτοὺ<supplied reason="lost">ς </supplied><gap reason="lost" quantity="1" unit="character"/><unclear>δι</unclear><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>

    <lb n="11" break="no"/>ναντο. ἀλλʼ <supplied reason="lost">ἐ</supplied>πε<supplied reason="lost">ι</supplied>δὴ οὐδεμία ἐπιστρέφια <app type="editorial"><lem resp="BL 1.207"><supplied reason="lost">κ</supplied>ατʼ <unclear>α</unclear><supplied reason="lost">ὐ</supplied>

    <lb n="12" break="no"/>τῶν</lem><rdg><unclear>ὑπὲ</unclear><supplied reason="lost">ρ αὐ</supplied>τῶν</rdg></app> γεγένηται, δ<unclear>ιὰ</unclear> τοῦ<unclear>το</unclear> πάλιν κατὰ τὴν πρόχ<unclear>θες</unclear> ἡ<supplied reason="lost">μέ</supplied>ρ<supplied reason="lost">αν</supplied> 

    <lb n="13"/>ἥτις ἐστὶν Παχὼν <gap reason="lost" quantity="2" unit="character"/><gap reason="lost" quantity="1" unit="character"/>ωδ<gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="1" unit="character"/>ου<gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="4" unit="character"/> 

    <lb n="14"/>συνηθῶς τὴ<unclear>ν</unclear> <gap reason="illegible" quantity="2" unit="character"/>ω<gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>οι<gap reason="illegible" quantity="1" unit="character"/>χ<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/> 

    <lb n="15"/>εἰς τὸ ἡμέτερον <app type="editorial"><lem resp="BL 1.207"><unclear>ὄ</unclear>ργα<unclear>νο</unclear><supplied reason="lost">ν</supplied> ἐξαφῆ<unclear>κα</unclear><supplied reason="lost">ν π</supplied>ρὸς <supplied reason="lost">πι</supplied>εῖν</lem><rdg><gap reason="lost" quantity="1" unit="character"/>ργα<gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"> ἐ</supplied>ξα<supplied reason="omitted">ί</supplied>φνης <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="2" unit="character"/><unclear>ι</unclear>ο<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ν</rdg></app> τ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> 

    <lb n="17"/>ἑαυτῶν ἐν τοῖ<supplied reason="lost">ς</supplied> <app type="editorial"><lem resp="BL 1.207">αὐτ<supplied reason="lost">οῖς πε</supplied>δίοις μου προσεποιησα<gap reason="lost" extent="unknown" unit="character"/></lem><rdg>ε<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="5" unit="character"/>διοις μ<unclear>ου</unclear><gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="2" unit="character"/></rdg></app> 

    <lb n="18"/>ὁ ἡμέτερος <app type="editorial"><lem resp="BL 1.207">ποι<supplied reason="lost">μὴν </supplied><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="3" unit="character"/>βιος τ<unclear>ὸ</unclear> <supplied reason="lost">ὄνο</supplied>μα</lem><rdg><gap reason="illegible" quantity="2" unit="character"/><unclear>ι</unclear><gap reason="lost" quantity="6" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="3" unit="character"/>βιος τ<unclear>ὸ</unclear> <gap reason="lost" quantity="5" unit="character"/><unclear>ια</unclear></rdg></app> ἐκ <gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="9" unit="character"/> 

    <lb n="19"/>οἱ δὲ <app type="editorial"><lem resp="BL 1.207">γυμνώ<supplied reason="lost">σαντες</supplied></lem><rdg>γυμν<unclear>ο</unclear><supplied reason="lost">ῦντες </supplied><gap reason="lost" quantity="3" unit="character"/></rdg></app> ε<supplied reason="lost">ὐθ</supplied>ὺς <app type="editorial"><lem resp="BL 1.207">μετὰ ῥοπαλῶν πρ<gap reason="lost" quantity="4" unit="character"/><unclear>ν</unclear><gap reason="illegible" quantity="3" unit="character"/></lem><rdg>μετ<gap reason="illegible" quantity="3" unit="character"/>παλων <gap reason="illegible" quantity="2" unit="character"/>ρ<gap reason="lost" quantity="4" unit="character"/><unclear>ν</unclear><gap reason="illegible" quantity="3" unit="character"/></rdg></app> 

    <lb n="20"/>τὴν ἐπικιμένην α<supplied reason="lost">ὐτοῦ ἐ</supplied>σθῆτα διαρ<supplied reason="lost">ή</supplied>ξαντες ἀφ<supplied reason="lost">εί</supplied>λαντ<supplied reason="lost">ο</supplied>,

    <lb n="21"/>ἔπειτα κατέκοψα<supplied reason="lost">ν</supplied> <unclear>π</unclear><supplied reason="lost">ληγ</supplied>αῖς αὐτὸν κατά τ<supplied reason="lost">ε</supplied> τῶν σκελῶν καὶ 

    <lb n="22"/>κατὰ τῶν ἄλλων μελ<unclear>ῶ</unclear><supplied reason="lost">ν</supplied> τοῦ σώματος, <app type="editorial"><lem resp="BL 1.207">ἡμιθανὴν</lem><rdg>ἡμιθανῆ</rdg></app> αὐτὸν <app type="editorial"><lem resp="BL 1.207">κατα

    <lb n="23" break="no"/>στήσαντες</lem><rdg><supplied reason="lost">πο</supplied><unclear>ι</unclear>ήσαντεσ</rdg></app> ὡς κα<supplied reason="lost">ὶ</supplied> φα<unclear>νε</unclear><supplied reason="lost">ρ</supplied>ά ἐστιν τὰ προσφωνηθέντα <unclear>ὑπὸ</unclear> 

    <lb n="24"/>τῶν ἐπιθεωρησάντων τὰ πλήγματα, ἀδήλου ο<supplied reason="lost">ὔ</supplied>σης τῆς 

    <lb n="25"/>ἑαυτοῦ σωτηρίας. διὰ το<supplied reason="lost">ῦτ</supplied>ο καὶ νῦν ἐπιδίδωμι τῇ σῇ λογιότητι 

    <lb n="26"/>τούσδε μου τοὺς λιβέλλου<supplied reason="lost">ς</supplied> μαρτυρόμενος καὶ <app type="editorial"><lem resp="BL 1.207">ἀξιῶν</lem><rdg>ἀξιῶ</rdg></app> τούτο<supplied reason="lost">υς</supplied> 

    <lb n="27"/>ἐν ἀσφαλεῖ εῖ̓ναι μέχρις τῆς εὐτυχοῦς ἐπιδημίας τοῦ κυρίου μου 

    <lb n="28"/>τοῦ ἄρχοντος· ἑτοιμότατα γὰρ ἔχω ἀπελ<unclear>έν</unclear>ξαι ἐν τῷ <app type="editorial"><lem resp="BL 1.207"><choice><reg>ἀχράν<supplied reason="omitted">τῳ</supplied></reg><orig>ἀχραν</orig></choice></lem><rdg>ἀχρ<supplied reason="lost">ά</supplied>ντῳ</rdg></app> αὐτοῦ 

    <lb n="29"/>δικαστηρίῳ τούτους λῃστὰς ὁμολό<supplied reason="lost">γ</supplied>ους καὶ ζῶα <choice><reg>ἀπελ<supplied reason="omitted">ηλ</supplied>ακότας</reg><orig>ἀπελακότας</orig></choice> 

    <lb n="30"/>πολλάκις καὶ Ἰωνᾶν τὸν προκίμενον φονέα ἀποδεῖξαι. 

    <lb n="31"/>μετὰ τὴν <expan>προκ<ex>ειμένην</ex></expan>, Παχὼν <app type="editorial"><lem resp="BL 1.207"><num value="10">ι</num></lem><rdg><num value="1">α</num></rdg></app>. <expan>Φλ<ex>άυιος</ex></expan> Ἰσίδωρος ὁ <expan>προκ<ex>είμενος</ex></expan> ἐπιδέδωκα. 
