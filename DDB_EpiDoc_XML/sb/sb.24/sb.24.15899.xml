         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/>ἐν ὀνόματι τῆς ἁγίας καὶ ὁμοουσί<supplied reason="lost">ου τριάδος πατρὸς <expan>κ<ex>αὶ</ex></expan></supplied> 

    <lb n="2"/>υἱοῦ <expan>κ<ex>αὶ</ex></expan> ἁγίου πνεύματος <expan>κ<ex>αὶ</ex></expan> τῆς δεσποίνη<supplied reason="lost">ς ἡμῶν τῆς θεοτόκου <expan>κ<ex>αὶ</ex></expan></supplied> 

    <lb n="3"/>πάντων τῶν <expan>ἁγίω<ex>ν</ex></expan>, βασιλείας τοῦ <expan>δεσπό<ex>του</ex></expan> <supplied reason="lost">ἡμῶν <expan>Φλ<ex>αουίου</ex></expan> Φωκᾶ τοῦ <expan>αἰων<ex>ίου</ex></expan></supplied> 

    <lb n="4"/>Αὐγούστου, ἔτους <num value="6">ϛ</num>, Μεσορὴ <num value="15">ιε</num>, <num value="12">ιβ</num> <expan>ἰν<ex>δικτίωνος</ex></expan>, <supplied reason="lost">ἐπʼ <expan>Ἀρσι<ex>νοιτῶν πόλεως</ex></expan></supplied>. 

    <lb n="5"/><handShift new="m2"/>τόδε τὸ κομπρόμισσον ποιοῦν<supplied reason="lost">ται πρὸς ἀλλήλους</supplied> 

    <lb n="6"/>ἑκουσίᾳ γνώμῃ ἐκ μὲν τοῦ ἑνὸς μ<supplied reason="lost">έρους Αὐρήλιος Κοσμᾶς</supplied>

    <lb n="7"/>ἐπικείμενος Στρατηγίου τοῦ πανευ<supplied reason="lost">φήμου πατρικίου</supplied> 

    <lb n="8"/>υἱὸς τοῦ τῆς μακαρίας μνήμης Ἀπο <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀπὸ τῆς <expan>Ἀρσι<ex>νοιτῶν</ex></expan> πόλεως</supplied>

    <lb n="9"/>ἐκ δὲ τοῦ ἑτέρου μέρους Αὐρηλία Μ<unclear>α</unclear><supplied reason="lost" cert="low">ροῦς θυγάτηρ Τι<unclear>μ</unclear></supplied><gap reason="lost" quantity="5" unit="character"/>

    <lb n="10"/>χωρ<supplied reason="lost">ὶς</supplied> <unclear>κυρί</unclear>ου ἀνδρ<unclear>ὸς</unclear> <unclear>χ</unclear><supplied reason="lost">ρ</supplied>η<supplied reason="lost">μ</supplied><unclear>α</unclear><supplied reason="lost">τ</supplied>ί<unclear>ζ</unclear><supplied reason="lost">ουσα ἀπὸ τῆς αὐτῆς</supplied>

    <lb n="11"/>πόλεω<supplied reason="lost">ς </supplied><gap reason="lost" extent="unknown" unit="character"/> 
<lb n="11"/><gap reason="lost" extent="unknown" unit="line"/> 
