    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/>τῆς χώρας περὶ τῆς τῶν δημοσί<supplied reason="lost">ων </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>α χρεῶν ἐστὶν ἐξωθηθῆναι <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>καὶ οἱ ταξεῶται πάντες <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><supplied reason="lost">δημ</supplied>οσίου παρενοχλοῦσίν μοι <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>δωρον παρασχεῖν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>ημένην παγαρχίαν κα<gap reason="lost" extent="unknown" unit="character"/>, 

    <lb n="7"/>βεβαιοῦνται δὲ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><abbr>ξ</abbr> ἐπιφερόμενοι <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>εν <choice><reg cert="low">λαργ<supplied reason="omitted" cert="low">ιτ</supplied>ιόνων</reg><orig>λαργιόνων</orig></choice> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>έναι ἀσφάλειαν ἐν τῷ πραιτωρίῳ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/><supplied reason="lost">ημ</supplied>ένης παγαρχ<supplied reason="lost">ίας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>τῆς προσόδου τοῦ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>ανοι μέντοι γε ἐγχώριοι <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/>τὴν <supplied reason="lost">ἀ</supplied>σφάλει<supplied reason="lost">αν</supplied> καὶ γι<gap reason="lost" extent="unknown" unit="character"/> 

