    <lb n="1"/><g type="chirho"/> Φλαουίῳ Ἰωάννῃ <gap reason="lost" quantity="15" unit="character"/> 

    <lb n="2"/>Στρατηγίῳ Ἰουλιαν<unclear>ῷ</unclear> <gap reason="lost" quantity="15" unit="character"/> 

    <lb n="3"/>κόμετι τοῦ θείου κ<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>ἐπαρχίας <handShift new="m2"/> <expan>π<ex>αρὰ</ex></expan> Φιλοξένου ἀρτοκόπου ἀπὸ τῆς <unclear>Ἀρ</unclear>σι<supplied reason="lost">νοιτῶν.</supplied> 

    <lb n="5"/>πάντων <choice><reg>ἀπολαυόντων</reg><orig>ἀπολαυωτων</orig></choice> τῆς καθαρᾶς δικαιοπραγίας 

    <lb n="6"/>τῆς ὑμετέρας ἐξουσίας, παρακαλῶ <choice><reg>καὶ ἐγὼ</reg><orig>κἀγὼ</orig></choice> 

    <lb n="7"/>μετὰ πάντων τοῦ δικαίου τυχεῖν, τοῦ ἐμοῦ <unclear>π</unclear><supplied reason="lost">ρά</supplied>

    <lb n="8" break="no"/>γματος ἐν τούτοις ὄντος· πρὸ δεκατεσσάρ<supplied reason="lost">ων</supplied> <num value="14"/> 

    <lb n="9"/>ἐτῶν ἠγαγόμην τὴν ἐμὴν σύμβιον Ἡραὶν καὶ <unclear>οὐκ</unclear> 

    <lb n="10"/>ἐ<supplied reason="lost">π</supplied><unclear>αυ</unclear>σάμην ἐγραζόμενο<supplied reason="lost">ς τὴν το</supplied><unclear>ῦ</unclear> ἀρτ<supplied reason="lost">οκόπου</supplied> 

    <lb n="11"/><supplied reason="lost">τέχνην</supplied> καὶ τὸ ἐξ αὐτῆς <gap reason="lost" quantity="19" unit="character"/> 

    <lb n="12"/><gap reason="lost" quantity="6" unit="character"/>ς ἐξέβαλε <gap reason="lost" quantity="22" unit="character"/> 

