         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><g type="stauros"/> τῷ <unclear>δ</unclear>ε<unclear>σπ</unclear>ότῃ ἡμῶ<supplied reason="lost">ν </supplied><gap reason="lost" quantity="7" unit="character"/><supplied reason="lost"> μεγαλο</supplied>

    <lb n="2" break="no"/>πρεπεστάτῳ καὶ ε<supplied reason="lost">ὐδοκιμωτάτῳ καὶ</supplied> 

    <lb n="3"/>περιβλέπτῳ <choice><reg>κόμ<supplied reason="lost">ετι</supplied></reg><orig>κώμ<supplied reason="lost">ετι</supplied></orig></choice> <gap reason="lost" quantity="11" unit="character"/> 

    <lb n="4"/>τῶν <choice><reg>σῶν</reg><orig>σο͂ν</orig></choice> δούλων πα<supplied reason="lost">ρὰ τῶν ἀπὸ κώμης</supplied> 

    <lb n="5"/>Δίννεως. θαυμά<unclear>ζ</unclear><supplied reason="lost">ομεν </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τὴν σὴν</supplied> 

    <lb n="6"/><choice><reg>μεγαλοπρέπειαν</reg><orig>μεγαλοπρέπιαν</orig></choice> φρω<supplied reason="lost">ντι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>Σολωμὼν εἰς <choice><reg>τὸν</reg><orig>των</orig></choice> κερ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>ἐξουσία καὶ πρὸ τῶν <expan>α<ex>ὐτῶν</ex></expan> δε<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>ἐγράψαμεν <choice><reg>τὸν</reg><orig>των</orig></choice> δεσπότ<supplied reason="lost">ην </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/><choice><reg>ἀνθρώπου</reg><orig>ἀνθρόπου</orig></choice> τούτου καὶ πά<unclear>λ</unclear><supplied reason="lost">ιν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>καὶ τάχα ῆ̓λθαν <expan>πάλι<ex>ν</ex></expan> τὰς κ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>κώμης καὶ ὅλοι οἱ ἀδελφο<supplied reason="lost">ὶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>εἰς <choice><reg>τὸ</reg><orig>τω</orig></choice> μέσον τοῦ κυρι<unclear>ζ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/><supplied reason="lost">α</supplied>ὐτῶν <gap reason="illegible" quantity="2" unit="character"/>μ<unclear>ε</unclear><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/><choice><reg>ἔχεις</reg><orig><supplied reason="lost">ἔ</supplied>χις</orig></choice> <gap reason="illegible" quantity="3" unit="character"/>τω<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/><unclear>ἐ</unclear>ξουσίαν <choice><reg>ἔχεις</reg><orig>ἔχις</orig></choice> αφον<gap reason="illegible" quantity="1" unit="character"/>εη<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/>περπατη<unclear>σε</unclear>ν εἰς τω<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="18"/>εἰ <choice><reg>δύναται</reg><orig>δήνατε</orig></choice> <choice><reg>πρωτεῦσαι</reg><orig>προτεῦσε</orig></choice> εἰ<unclear>ς</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="19"/>φιλις <choice><reg>φρόντισεν</reg><orig>φ<unclear>ρ</unclear>ώντισεν</orig></choice> <unclear>τ</unclear>οὺ<unclear>ς</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="20"/>εἰς <choice><reg>τὸν</reg><orig>των</orig></choice> <choice><reg>ἑξῆς</reg><orig>ἑξῦς</orig></choice> χάριν τῆ<unclear>ς</unclear> μυή<supplied reason="lost">σεως </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="21"/>τῇ κώμῃ καὶ τὸν τύπον <choice><reg>ὃν</reg><orig>τον</orig></choice> ἔδω<unclear>κ</unclear>εν <unclear>τῇ</unclear> κώμῃ 

    <lb n="22"/>καὶ ἄρτι χάριν <choice><reg>τὸ</reg><orig>τω</orig></choice> <choice><reg>συμφέρον</reg><orig>συμφέρων</orig></choice> <unclear>ἡμῖν</unclear> θέλο

    <lb n="23" break="no"/>μεν αὐτὸν ἕως τοῦ σώματος τ<unclear>οῦ</unclear> δεσπότου 

    <lb n="24"/>ἡμῶν, ἐὰν <choice><reg>θέλῃς</reg><orig>θέλις</orig></choice> εγιρε τὴν <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><unclear>ε</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="25"/>αν εἰς τὴν κώμην, εἰ <choice><reg>δήλωσον</reg><orig>δύλωσον</orig></choice> α<supplied reason="lost">ὐτῷ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost">, χρεία</supplied> 

    <lb n="26"/>γάρ ἐστιν τοῦ σίτου καὶ το<unclear>ῦ</unclear> ὕδ<supplied reason="lost">ατος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="27"/>Μωσῆς ὡς κελεύεις <unclear>ἐ</unclear>ξουσία<unclear>ν</unclear> ἐχ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="28"/>καὶ τὸν θεόν σου, <choice><reg>χάρισον</reg><orig>χάρισεν</orig></choice> ἡμῖν τὰ φο<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="29"/>ταῦτα, δέσποτα ἡμῶν <gap reason="lost" extent="unknown" unit="character"/> 

