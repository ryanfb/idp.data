         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/><g type="stauros"/> ἐν ὀνόματι τοῦ θεοῦ. Ἄμβρος <expan>σύμβ<ex>ου</ex>λ<ex>ος</ex></expan> τῷ <expan>παγ<ex>άρ</ex>χ<ex>ῳ</ex></expan> τῆς Ἡρακλέους.

    <lb n="2"/><expan>παρ<ex>άσ</ex>χ<ex>ες</ex></expan> Ἄμερ <expan>υἱ<ex>ῷ</ex></expan> Ἄσλα <expan><ex>ὑπὲρ</ex></expan> <expan>νο<ex>μισματίων</ex></expan> <num value="2">β</num> <expan>χόρτ<ex>ου</ex></expan> <expan>δέ<ex>σμας</ex></expan> <supplied reason="lost">τ</supplied>έσσαρας <num value="4"/> <expan>μό<ex>νας</ex></expan> <expan>λαμβάνω<ex>ν</ex></expan> παρʼ <expan>αὐτ<ex>οῦ</ex></expan>

    <lb n="3"/>τὸ <expan>λογάρ<ex>ιον</ex></expan> <expan><ex>καὶ</ex></expan> <expan><ex>ὑπὲρ</ex></expan> <expan>δαπα<ex>νῶν</ex></expan> <expan>ἀνθ<ex>ρώπων</ex></expan> αὐτοῦ <expan>ἑκ<ex>άσ</ex>τ<ex>ῳ</ex></expan> τῷ <expan>μ<ex>ηνὶ</ex></expan> <expan>ψωμίω<ex>ν</ex></expan> <expan>ἀρτ<ex>άβην</ex></expan> μίαν <num value="1"/> <expan>μό<ex>νην</ex></expan>· <expan>κατάγρα<ex>ψον</ex></expan>

    <lb n="4"/>αὐτὰ <expan><ex>καὶ</ex></expan> τοῦτον δὲ ἐπίστειλον <expan>εἰ<ex>ς</ex></expan> <expan>χῶ<ex>ρον</ex></expan> <expan>πεποιημέ<ex>νον</ex></expan> <expan><ex>καὶ</ex></expan> μὴ <expan>βαρέσ<ex>ης</ex></expan> <expan>χῶ<ex>ρον</ex></expan> ὑπὲρ ἄλλου.

    <lb n="5"/><expan>ἐγρά<ex>φη</ex></expan> <expan>μ<ex>ηνὸς</ex></expan> <expan>Τυ<ex>βὶ</ex></expan> <num value="13">ιγ</num> <expan>ἰνδ<ex>ικτίονος</ex></expan> <expan>πρώτη<ex>ς</ex></expan>. <g type="stauros"/>
