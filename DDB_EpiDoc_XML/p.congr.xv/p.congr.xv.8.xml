         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/><supplied reason="lost" cert="low">Φιλῖν</supplied>ος Ζήνωνι χαίρειν. γράφεις <supplied reason="lost">μοι ἵνα </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="2"/><supplied reason="lost">φροντί</supplied>σαι τὸν Μόσχον. ἐγὼ δὲ παραγεν<supplied reason="lost">όμενος </supplied><gap reason="lost" quantity="3" unit="character"/>

    <lb n="3"/><supplied reason="lost">ἐν τάχε</supplied>ι κατελάμβανον ὄντα παρὰ Πτ<supplied reason="lost" cert="low">ολεμαιου  </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="4"/><supplied reason="lost">ἀπαχθέντος αὐτ</supplied>οῦ εἰς τὸ δεσμωτήριον συν<unclear>έ</unclear><supplied reason="lost">χεσθαι </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/>ε τῆι <num value="19">ιθ</num> παραγενόμενον αὐτ<supplied reason="lost">όν </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="6"/><gap reason="lost" extent="unknown" unit="character"/><unclear>ε</unclear>ναι αὐτοῖς ἐπὶ τοῦ παρόντος ε<unclear>π</unclear><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="7"/><supplied reason="lost">παραγεν</supplied>όμενος τῆι <num value="19">ιθ</num> διακριθήσετα<supplied reason="lost">ι ἐπὶ τοῦ κριτηρίου </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="8"/><supplied reason="lost">εἰ δοκεῖ</supplied> <unclear>σ</unclear>οι, παρέσομαι δὲ καὶ αὐτὸς <unclear>τ</unclear><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="9"/><gap reason="lost" extent="unknown" unit="character"/>ως ἐπιστρέφεις παραδ<gap reason="lost" extent="unknown" unit="character"/>
</ab></div><div n="v" type="textpart"><!--milestone unit="4"--><ab>

