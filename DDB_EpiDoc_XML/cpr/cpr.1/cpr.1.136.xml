    <lb n="1"/><supplied reason="lost">ἀπεσχη</supplied>κέν<supplied reason="lost">αι π</supplied>αραχρῆμα τὸ λ<supplied reason="lost">οιπὸ</supplied>ν διὰ τραπέζης. περὶ δὲ τ<supplied reason="lost">οῦ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>γεγενῆσθαι ἐπερωτηθέντες ὡμολόγησαν.  ὁ δὲ χρ<supplied reason="lost">ηματισμὸς</supplied> 

    <lb n="3"/>ἐτελειώθη κινδύνῳ αὐτῶν πρωτοπραξίας οὔσης τῷ δημ<supplied reason="lost">οσίῳ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>κυρια.  <handShift new="m2"/> Αὐρηλία Ἄρτεμις ἡ καὶ Κύριλλα μετὰ κυρίου τοῦ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/><supplied reason="lost">Αὐρ</supplied>ηλίου Ἀπολλωνίου πέπρακα τὰς προ<supplied reason="lost">κ</supplied>ι<supplied reason="lost">μένας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/> <supplied reason="lost">ἀπὸ τοῦ νῦν</supplied> ἐφʼ ἅπαντα χρόνον <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>τὸ παρα<supplied reason="lost">χωρητικὸν ἅπαν ὅ ἐσ</supplied>τι δραχμ<supplied reason="lost">αὶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><gap reason="lost" quantity="16" unit="character"/><supplied reason="lost"> ὡς πρόκειτ</supplied>αι.  <handShift new="m3"/> Αὐρηλία 

    <lb n="9"/><gap reason="lost" quantity="16" unit="character"/><supplied reason="lost"> παρεχ</supplied>ώρησα τὴν ἄρουρ<supplied reason="lost">αν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

