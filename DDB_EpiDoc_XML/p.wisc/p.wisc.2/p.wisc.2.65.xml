    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/>ἀπὸ τῆς μεγάλης <gap reason="lost" quantity="30" unit="character"/> 

    <lb n="2"/>κυριας Αὐρηλίῳ <gap reason="lost" quantity="25" unit="character"/><supplied reason="lost">  ἀπὸ τῆς</supplied> 

    <lb n="3"/><unclear>Ὀξυρ</unclear>υγχιτῶν <supplied reason="lost">πόλεως χαίρειν. ὁμολογῶ παρειλη</supplied>

    <lb n="4" break="no"/><unclear>φ</unclear>έναι παρὰ σοῦ καὶ <unclear>ἐ</unclear><supplied reason="lost">μβεβλῆσθαι </supplied><gap reason="lost" quantity="20" unit="character"/> 

    <lb n="5"/>ἐν ὅρμῳ τῆς Ὀξυρ<unclear>υ</unclear><supplied reason="lost">γχιτῶν πόλεως  </supplied><gap reason="lost" quantity="17" unit="character"/> 

    <lb n="6"/>δεκαπέντε <num value="15"/> ναύλου αὐτῶν χ<unclear>ρυ</unclear><supplied reason="lost">σοῦ </supplied><gap reason="lost" quantity="17" unit="character"/> 

    <lb n="7"/>ἐπὶ τῷ με τὸν γόμον ἀποκομίσ<unclear>ε</unclear><supplied reason="lost">σθαι εἰς τὴν <expan>λαμπρ<ex>οτάτην</ex></expan> Ἀλεξάν</supplied>

    <lb n="8" break="no"/>δρειαν καὶ παραδοῦναί σοι ἄδολ<supplied reason="lost">ον καθαρὸν ἀκακούργητον</supplied> 

    <lb n="9"/>ἀπὸ πάσης ναυτικῆς κακουργίας <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="20" unit="character"/><supplied reason="lost">  καὶ</supplied> 

    <lb n="10"/>ἅμα τῇ <choice><reg>ἀπογομώσει</reg><orig>ἀπογομώσι</orig></choice> <choice><reg>λαβεῖν</reg><orig>λαβῖν</orig></choice> με παρ<supplied reason="lost">ὰ σοῦ </supplied><gap reason="lost" quantity="20" unit="character"/> 

    <lb n="11"/><expan><ex>τάλαντα</ex></expan> <num value="8">η</num> ναυλωτικῆς. ἐπερωτηθε<supplied reason="lost">ὶς ὡμολόγησα</supplied>. 

    <lb n="12"/>Αὐρήλιος Κολλοῦθος Πάτος <gap reason="lost" quantity="20" unit="character"/> 

