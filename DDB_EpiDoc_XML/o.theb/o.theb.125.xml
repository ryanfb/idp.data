    <lb n="1"/><supplied reason="lost"><expan>μέ<ex>τρημα</ex></expan></supplied> <expan>θησ<ex>αυροῦ</ex></expan> <expan>μη<ex>τροπόλεως</ex></expan> <expan>γενή<ex>ματος</ex></expan> <num value="2">β</num> <expan><ex>ἔτους</ex></expan> τῶν κυρίων ἡμῶν 

