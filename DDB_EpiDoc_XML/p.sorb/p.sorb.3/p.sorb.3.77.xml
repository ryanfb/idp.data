         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><supplied reason="lost">Διονύσιος</supplied> Ἀριστάρχωι χα<supplied reason="lost">ίρειν. </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>ὁ σιτολόγος τὰ εἰσαγόμενα δ<unclear>ι</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>εἰς τὸν σπόρον τοῦ <num value="39">λθ</num> <expan><ex>ἔτους</ex></expan> τ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><choice><reg>ἐν</reg><orig>ἐμ</orig></choice> Πτολεμαίδι πρὸς τῆι π<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>καθάπερ πρότερον πλεονάκ<supplied reason="lost">ις γέγραφα </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>καὶ τοῖς γεωργοῖς ἀνεδίδοτ<supplied reason="lost">ο </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>ποιεῖσθαι καὶ εἴπερ τι τοιοῦτο <gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">οὐκ</supplied>

    <lb n="8"/>ἠγνόηκας τοῦ συμφέροντος <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>τῶν πρότερον πειραθῆτε <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>τοῦ Ἀριστίωνος σύμβολα τ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>προέσθαι τὸ κατὰ γεωργὸν πλ<unclear>ῆ</unclear><supplied reason="lost">θος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>τὰ σπέρματα παραμετρ<unclear>ῆ</unclear><supplied reason="lost">σαι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>τὴν ἐξανήλωσιν ποιήσει <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἐν</supplied>

    <lb n="14"/>ἐγκλήμασιν ἔσει ἀπολογίσα<unclear>ι</unclear> <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> διὰ τίνα</supplied> 

    <lb n="15"/>αἰτίαν οὐκ ωἰκονόμησας ω<gap reason="lost" extent="unknown" unit="character"/> 
</ab></div><div n="v" type="textpart"><ab>

    <lb n="16"/><expan><ex>ἔτους</ex></expan> <app type="editorial"><lem><num value="38">λη</num></lem><rdg resp="SB 12 10850"><num value="31">λα</num></rdg></app> Θωὺθ <num value="6">ϛ</num> με<gap reason="illegible" quantity="1" unit="character"/> 

    <lb n="17"/>Διονύσιος τ<unclear>ῆι</unclear> ἐπιστολ<unclear>ῆι</unclear> <gap reason="illegible" quantity="6" unit="character"/> 

    <lb n="18"/><app type="editorial"><lem>παρ’ Ἀριστίωνος <gap reason="illegible" quantity="5" unit="character"/></lem><rdg resp="SB 12 10850"><gap reason="illegible" quantity="1" unit="character"/><unclear>αρις</unclear> <unclear>Πτολε</unclear>μαίδι</rdg></app> ἀπὸ <unclear>τ</unclear>ο<unclear>ῦ</unclear>   Ἀριστάρχωι 

