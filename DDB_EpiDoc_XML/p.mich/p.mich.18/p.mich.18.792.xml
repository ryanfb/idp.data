         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="1" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/><supplied reason="lost">Αὐρήλιο</supplied>ς Φιλώτα<unclear>ς</unclear> ἀπά<supplied reason="lost">τ</supplied><unclear>ωρ</unclear> <unclear>μ</unclear><supplied reason="lost">ητρὸς</supplied>

    <lb n="2"/>Ταηρ<supplied reason="lost">ακλ</supplied>είδου ἀπὸ Ὀξυρύγχων <expan>πόλ<supplied reason="lost"><ex>εως</ex></supplied></expan><supplied reason="lost">, καταγι</supplied>

    <lb n="3" break="no"/>νόμεν<supplied reason="lost">ος</supplied> ἐν Σερύφει, Αὐρηλίᾳ Ἡ<unclear>ρα</unclear><supplied reason="lost">ίδι θυγα</supplied>

    <lb n="4" break="no"/>τρὶ Διογέ<supplied reason="lost">ν</supplied>ους ἐξηγητ<supplied reason="lost">εύ</supplied>σαντος τῆς α<supplied reason="lost">ὐτῆς πό</supplied>

    <lb n="5" break="no"/>λεως διʼ Α<supplied reason="lost">ὐ</supplied>ρηλίου <unclear>Ερ</unclear><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="3" unit="character"/> χ<unclear>αί</unclear>

    <lb n="6" break="no"/>ρειν. ὁμο<supplied reason="lost">λ</supplied>ογῶ παρει<supplied reason="lost">λ</supplied>ηφέναι παρὰ <supplied reason="lost">σοῦ ἐν</supplied>

    <lb n="7"/><choice><reg>προχρε<supplied reason="lost">ί</supplied>ᾳ</reg><orig>προχρ<supplied reason="lost">ι</supplied>ᾳ</orig></choice> πρὸς ὑπηρεσίαν <unclear>π</unclear>οτισ<unclear>μ</unclear><supplied reason="lost">ῶν</supplied>

    <lb n="8"/>οῦ̔ ἔχω ν<unclear>ῦ</unclear>ν μισθώσει ἀμπελικοῦ <supplied reason="lost">κτή</supplied>

    <lb n="9" break="no"/>ματος Νήσου καλουμένου περὶ π<supplied reason="lost">όλιν</supplied>

    <lb n="10"/>ἀκολού<unclear>θ</unclear>ως ῇ̔ ἔχω μ<supplied reason="lost">ι</supplied>σθώσει βο<unclear>ϊ</unclear><supplied reason="lost">κὰ κτή</supplied>

    <lb n="11" break="no"/>νη ἐν συ<unclear>ν</unclear>τιμήσει δ<supplied reason="lost">ραχ</supplied>μῶν <choice><reg>χι<supplied reason="lost">λίων</supplied></reg><orig>χει<supplied reason="lost">λίων</supplied></orig></choice>

    <lb n="12"/>πεντακοσίων <num value="1500"/>, <unclear>ἔ</unclear><supplied reason="lost">τι δ</supplied>ὲ καὶ ἐσχη<supplied reason="lost">κέναι, καὶ</supplied>

    <lb n="13"/>ἀπὸ τῶν <supplied reason="omitted">διὰ</supplied> τῆς αὐτῆ<unclear>ς</unclear> <supplied reason="lost">μισ</supplied><unclear>θ</unclear>ώσεως δ<supplied reason="lost">ηλουμέ</supplied>

    <lb n="14" break="no"/>νων λα<unclear>β</unclear>εῖν με παρ<supplied reason="lost">ὰ σο</supplied>ῦ ὁμοίως ε<supplied reason="lost">ἰς τὸ</supplied>

    <lb n="15"/>κατὰ μέρ<unclear>ο</unclear><supplied reason="lost">ς</supplied> δραχμῶν <choice><reg>χιλίων</reg><orig>χειλίων</orig></choice> πε<supplied reason="lost">ντακο</supplied>

    <lb n="16" break="no"/>σίων <num value="1500"/> <unclear>τ</unclear>ὰς τῆς πρώ<supplied reason="lost">τ</supplied>ης δόσεως τ<supplied reason="lost">οῦ ὄντος</supplied>

    <lb n="17"/>μηνὸς <unclear>Ἁ</unclear>θὺρ δραχμ<unclear>ὰ</unclear><supplied reason="lost">ς</supplied> <choice><reg>πεντακ<unclear>ο</unclear><supplied reason="lost">σίας</supplied></reg><orig>πεντακ<unclear>ω</unclear><supplied reason="lost">σίας</supplied></orig></choice><supplied reason="lost"> <num value="500"/></supplied>

    <lb n="18"/>καὶ πάντα ἀποδώσειν ἀκολούθω<supplied reason="lost">ς τῇ</supplied>

    <lb n="19"/>μισθώ<unclear>σ</unclear>ει, τὰ δὲ κτή<supplied reason="lost">ν</supplied>η ἐπὶ τῇ περ<supplied reason="lost">ὶ αὐ</supplied>

    <lb n="20" break="no"/>τῶν <choice><reg>ἐγγραμμένῃ</reg><orig>ενγεγραμμενῃ</orig></choice> αἱρέσει, μενο<supplied reason="lost">υσῶν</supplied>

    <lb n="21"/>κυρίω<supplied reason="lost">ν</supplied> τῆς τε αὐτῆς μισθώσεω<supplied reason="lost">ς καὶ</supplied>

    <lb n="22"/>ῆ̔ς ἄλλη<supplied reason="lost">ς</supplied> ἔχω μισθώσεως ποτισμ<supplied reason="lost">ῶν</supplied>

    <lb n="23"/>ἑτέρου σ<unclear>ο</unclear>υ ἀμπελικοῦ κτήματος ἐφʼ <supplied reason="lost">οῖ̔ς</supplied>

    <lb n="24"/>ἑκατέρα περιέχει πᾶ<unclear>σι</unclear>, <choice><reg>γινομένης</reg><orig>γεινομέν<supplied reason="lost">ης</supplied></orig></choice>

    <lb n="25"/>τῆς πρ<unclear>ά</unclear>ξεως παρά τε ἐμοῦ καὶ ἐκ τ<supplied reason="lost">ῶν ὑ</supplied>

    <lb n="26" break="no"/>παρχόν<unclear>τ</unclear>ων μοι πάντων. κύρι<supplied reason="lost">α τάδε</supplied>

    <lb n="27"/>τὰ γράμματα δισσὰ γραφέντα· περὶ δὲ τοῦ <supplied reason="lost">ταῦτα</supplied>

    <lb n="28"/>ὀρθῶς καλῶς <choice><reg cert="low">γίνεσθαι </reg><orig>γείνεσθαι</orig></choice> ἐπερωτηθεὶ<supplied reason="lost">ς ὡμο</supplied>

    <lb n="29" break="no"/>λόγησα. <expan><ex>ἔτους</ex></expan> <num value="5">ε</num> Αὐτοκράτορος Καίσ<unclear>α</unclear><supplied reason="lost">ρος</supplied>

    <lb n="30"/>Μάρκου Αὐρηλίου Ἀν<unclear>των</unclear>ινου Εὐσεβ<supplied reason="lost">οῦς</supplied>

    <lb n="31"/>Εὐτυχοῦ<supplied reason="lost">ς</supplied> καὶ Μάρκου Αὐρηλίου Ἀλεξάν<supplied reason="lost">δρου</supplied>

    <lb n="32"/>Καίσαρ<unclear>ο</unclear><supplied reason="lost">ς</supplied> <unclear>Σ</unclear>εβαστῶν Ἁθύ<supplied reason="lost">ρ</supplied> <num value="2">β</num> <handShift new="m2"/>Αὐ<supplied reason="lost">ρήλιος</supplied>

    <lb n="33"/>Φι<supplied reason="lost">λώτ</supplied><unclear>ας</unclear> <unclear>π</unclear><supplied reason="lost">αρεί</supplied>

    <lb n="34" break="no"/>ληφ<supplied reason="lost">α ὡς</supplied> <choice><reg>π<supplied reason="lost">ρό</supplied>κ<supplied reason="lost">ειται</supplied></reg><orig><unclear>π</unclear><supplied reason="lost">ρο</supplied><unclear>χ</unclear><supplied reason="lost">ειται.</supplied></orig></choice></ab></div></div>
