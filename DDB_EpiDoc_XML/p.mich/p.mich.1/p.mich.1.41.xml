    <lb n="5"/><unclear>χ</unclear>ρή<unclear>σας</unclear> <gap reason="illegible" quantity="2" unit="character"/> παρʼ ἡμῶν, 

    <lb n="6"/>εἰ τ<gap reason="illegible" quantity="1" unit="character"/>ν <unclear>απ</unclear>αν<gap reason="illegible" quantity="4" unit="character"/>ρ<unclear>αιμ</unclear><gap reason="illegible" quantity="2" unit="character"/> 

